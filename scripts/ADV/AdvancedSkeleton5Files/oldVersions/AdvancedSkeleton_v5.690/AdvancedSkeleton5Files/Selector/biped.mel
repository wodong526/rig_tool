setParent..;
scrollField -wordWrap false -text "Dialog text here" asAutoLipSyncUIScrollField;
frameLayout -w 300 -cll 1 -cl 1 -l "advanced options" asAutoLipSyncUIFrameLayout;
	columnLayout -adj 1;
